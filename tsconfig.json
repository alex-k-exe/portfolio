{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "strictNullChecks": true,
    "paths": {
      "@components/*": ["./src/components/*"],
      "@content/*": ["./src/content/*"],
      "@layouts/*": ["./src/layouts/*"],
      "@src/*": ["./src/*"]
    }
  }
}
